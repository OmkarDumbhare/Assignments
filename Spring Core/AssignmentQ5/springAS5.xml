<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
                           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                           http://www.springframework.org/schema/context
                           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
                           
<bean id="streq" class="core_assignment.Player">
<property name="Name" value="Justin"></property>
</bean> 

<bean id="stres" class="core_assignment.Player">
<property name="Name" value="Tyson"></property>
<property name="Age" value="27"></property>
</bean>                 


<bean id="team" class="core_assignment.Team">
<property name="teamName" value="Golden State Warriors"></property>
<property name="totalPlayers" value="22"></property>
</bean>         

<bean id="team1" class="core_assignment.Team">
<property name="teamName" value="Sacremento Kings"></property>
<property name="totalPlayers" value="24"></property>
</bean>         

<bean id="team2" class="core_assignment.Team">
<property name="teamName" value="Denver Nuggets"></property>
<property name="totalPlayers" value="21"></property>
</bean>         

<bean id="team3" class="core_assignment.Team">
<property name="teamName" value="Atlanta Hawks"></property>
<property name="totalPlayers" value="25"></property>
</bean>         

                           
<bean class="org.springframework.beans.factory.annotation.RequiredAnnotationBeanPostProcessor"/>  
  <bean class="org.springframework.context.annotation.CommonAnnotationBeanPostProcessor"/>                           
                            
                           
                           
                         
                           
</beans>