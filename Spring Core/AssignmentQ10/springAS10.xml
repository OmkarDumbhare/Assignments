<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
                           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                           http://www.springframework.org/schema/context
                           http://www.springframework.org/schema/context/spring-context-3.0.xsd">

                           
    <bean id="contextAware" class="core_assignment.ContextAware">
        <property name="place" value="California"/>
        <property name="demo" ref="Demo"/>
    </bean>
    <bean id="contextAware1" class="core_assignment.ContextAware">
        <property name="place" value="BrooklynS"/>
        <property name="demo" ref="Demo"/>
    </bean>

    <bean id="Demo" class="core_assignment.Demo" scope="prototype">
        <property name="x" value="10"/>
        <property name="y" value="20"/>
    </bean>
                           
                           
                           
                           
                           </beans>