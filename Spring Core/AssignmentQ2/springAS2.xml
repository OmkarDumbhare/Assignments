<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
                           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                           http://www.springframework.org/schema/context
                           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
                           
<bean id="question" class="core_assignment.Question">
<property name="questionId" value="1"></property>
<property name="question" value=" NBA TEAMS"></property>
<property name="answersList">
   <list>
     <value>Golden State Warriors</value>
     <value>LosAngeles Lakers</value>
     <value>Phoenix Suns</value>
   </list>
</property>
</bean>

<bean id="question2" class="core_assignment.Question">
<property name="questionId" value="1"></property>
<property name="question" value=" NBA TEAMS"></property>
<property name="answersSet">
   <set>
     <value>Golden State Warriors</value>
     <value>LosAngeles Lakers</value>
     <value>Phoenix Suns</value>
   </set>
</property>
</bean>

<bean id="question3" class="core_assignment.Question">
<property name="questionId" value="0"></property>
<property name="question" value=" NBA TEAMS"></property>
<property name="answersMap">
  <map>
     <entry key="1" value="Golden State Warriors"></entry>
     <entry key="2" value="LosAngeles Lakers"></entry>
     <entry key="3" value="Phoenix Suns"></entry>
  </map>
</property>
</bean>
                              
</beans>